{
  "name": "graphql-helix",
  "version": "1.8.3",
  "description": "A highly evolved GraphQL HTTP Server ðŸ§¬",
  "keywords": [
    "graphql",
    "graphiql",
    "server",
    "http",
    "sse",
    "multipart",
    "defer",
    "stream"
  ],
  "homepage": "https://github.com/contrawork/graphql-helix",
  "bugs": {
    "url": "https://github.com/contrawork/graphql-helix/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contrawork/graphql-helix.git"
  },
  "license": "MIT",
  "author": "Daniel Rearden <danielrearden@gmail.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "dist-esm"
  ],
  "scripts": {
    "build": "yarn build-render-graphiql && yarn build-deno && yarn run build-node",
    "build-deno": "denoify && del-cli ../deno --force && move-file deno_dist ../deno && cpy ../../README.md ../deno",
    "build-node": "del-cli dist && del-cli dist-esm && tsc && tsc -b tsconfig.esm.json && node scripts/esm-post-process.js",
    "build-render-graphiql": "cd ../graphiql && yarn run build",
    "test": "jest test/integration.test.ts",
    "esm:test": "node scripts/test-esm.mjs",
    "dev": "yarn ts-node-dev dev-server/index.ts",
    "e2e": "yarn e2e-server 'cypress run'",
    "e2e-server": "start-server-and-test 'yarn dev' 'http-get://localhost:4000/graphql?query={__typename}'"
  },
  "devDependencies": {
    "@types/chance": "1.1.3",
    "@types/eventsource": "1.1.6",
    "@types/jest": "27.0.2",
    "chance": "1.1.8",
    "chalk": "4.1.2",
    "cpy-cli": "3.1.1",
    "cypress": "8.6.0",
    "del-cli": "4.0.1",
    "denoify": "0.9.1",
    "eventsource": "1.1.0",
    "get-port": "5.1.1",
    "glob": "7.2.0",
    "globby": "12.0.2",
    "got": "11.8.2",
    "graphql": "15.4.0-experimental-stream-defer.1",
    "husky": "7.0.2",
    "jest": "27.2.1",
    "lint-staged": "11.1.2",
    "move-file-cli": "3.0.0",
    "replacestream": "4.0.3",
    "start-server-and-test": "1.14.0",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "ts-node-dev": "1.1.8",
    "typescript": "4.4.3"
  },
  "peerDependencies": {
    "graphql": "^15.3.0"
  },
  "denoify": {
    "replacer": "scripts/denoify-replacer.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist-esm/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist-esm/*.mjs"
    },
    "./send-result/node-http": {
      "require": "./dist/send-result/node-http.js",
      "import": "./dist-esm/send-result/node-http.mjs"
    }
  }
}
